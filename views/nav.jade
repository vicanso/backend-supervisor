- var activePage = (viewData && viewData.page) || 'home';
nav.navContainer
  ul
    li(class='#{activePage === "home"? "active" : ""}'): a.text-center(href='#{URL("/")}', title='Supervisor') 
      span.glyphicons.glyphicons-home
      | Home
    li(class='#{activePage === "backend"? "active" : ""}'): a.text-center(href='#{URL("/backend")}', title='backend')
      span.glyphicons.glyphicons-server
      | Backend
    li(class='#{activePage === "varnish"? "active" : ""}'): a.text-center(href='#{URL("/varnish")}', title='varnish')
      span.glyphicons.glyphicons-magic
      | Varnish
    
    li(class='#{activePage === "log"? "active" : ""}'): a.text-center(href='#{URL("/log")}', title='log')
      span.glyphicons.glyphicons-random
      | Log
- importer.import('/css/nav.css');
