extends ./layout
block bodyContainer
  .homePage(ng-controller='HomePageController')
    include ./nav
    header.headerContainer
      ul.pull-right.functions
        li.login: a(href='javascript:;', ng-click='app.login()') 登录
        li.register: a(href='javascript:;', ng-click='app.register()') 注册
        li.setting
          a.glyphicons.glyphicons-menu-hamburger.font16(href='#{URL("/setting")}')
      h3.text-center Supervisor

  
  
  - importer.import('/js/home.js');
  - importer.import('/css/home.css');