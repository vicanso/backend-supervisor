@import 'nib'
@import './common'

logViewContaienrCss()
  jt-log-list
    display block
    background-color #1e1e1e

  ul
    list-style none
    padding 0
    margin 0
    li
      padding 0
      margin 0
      color #529B2F
      span
        margin-left 10px
      .timestamp
        color #e6db74
      .level
        color #66d9ef
        &.warn
          color #fd971f
        &.error
          color #f31b14

.logPage
  margin-left $NAV_WIDTH
  .headerContainer
    headerContainerCss()

  .logViewContaienr
    logViewContaienrCss()
